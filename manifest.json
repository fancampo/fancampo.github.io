    "^#(.+)~edit$": "#$1",
    "#iconos": "./assets/iconos.resx",
    "#site": "./assets/site.resx",
    "#menu": "./assets/menu.xml",
    "#nuestra_gestion": "./assets/nuestra_gestion.resx",
    "#aviso_privacidad": "./assets/aviso_privacidad.resx",
    "#caracteristicas": "./assets/caracteristicas.resx",
